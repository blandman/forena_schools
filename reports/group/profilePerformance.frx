<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<title>Sample Group Profile Performance</title>
<frx:category>Student</frx:category>
<frx:options />
<frx:fields>
	  <frx:field id="export" link="report_doc/group.profileGraph.csv?group_id={parm.group_id}&amp;profile_id={parm.profile_id}" >Export Data</frx:field>
</frx:fields>
<frx:parameters>
  <frx:parm id="bldg_id" label="Building" type="selectajax" data_source="ims/buildings" require="1"/>
  <frx:parm id="grade_level" label="Grade Level" type="selectajax" data_source="ims/building/grades" />
  <frx:parm id="school_year" label="School Year" type="selectajax" data_source="ims/school_years" /> 
  <frx:parm id="group_id" label="Course" type="selectajax" data_source="ims/building/courses_for_grade_level"/>
	<frx:parm id="profile_id"  label="Profile"  type="select" data_source="ims/group/performance_profiles" data_field="profile_id" label_field="name"/> 
</frx:parameters>
<frx:docgen />
<!-- Uncomment the following to add this to the group tabs
<frx:menu path="group/:group_id/profile" type="local-task" title="Performance"/>
-->
</head>
<body>
 <div frx:block="ims/group/details" id="group" frx:foreach="*" class="grid_12">
   <h1 frx:renderer="FrxTitle">{group.name}, {group.year_label}</h1>
   <h4>{first_name} {last_name}</h4>
 </div>
	<div frx:renderer="FrxParameterForm" frx:collapsible='0' frx:title="Select Profile"/>
  <div id="profile" frx:block="ims/profiles/profile" frx:foreach="*">
    <h2>{name} </h2>
  </div>
  <div class="graph"
  id="profile_performance_block" 
      frx:block="ims/group/profile_performance" >
  <svg 
      frx:type="scatterplot" 
      frx:renderer="FrxSVGGraph"
      frx:series_1="{norm_score}"
      frx:seriesx_1="{grade}"
      frx:best_fit="straight"
      frx:label_x="Grade Level"
      frx:label_y='Norm Score'
      frx:width="600" frx:height="400"
      />
  </div>
  <div frx:block="ims/group/profile_performance_summary" frx:foreach="*">
	  <h3>Summary</h3> 
	  <table style="width: 30em; ">
	  <tr>
		  <th>Assessements</th><td>{assessments}</td>
	  </tr>
	  <tr>
		  <th>Students</th><td>{students}</td>
	  </tr>
	  <tr>
		  <th>Starting</th><td>{start_date}</td>
	  </tr>
	  <tr>
	    <th>Ending</th><td>{end_date}</td>
	  </tr>	  
	  </table>
	  <p>{export}</p>
  </div>
  
  

</body>
</html>

<html xmlns:frx="urn:FrxReports">
<head>
<title>Profile Definitions</title>
<frx:fields>
  <frx:field id="name" link="index.php?q=ProfileEditor&amp;profile_id={profile_id}&amp;popup=true" target="popup{profile_id}"/>
  <frx:field id="add" link="index.php?q=ProfileEditor&amp;popup=true" target="popupNewProfile">Add Assessment Profile</frx:field>
</frx:fields>
</head>
<body>
  <div>
  <p>{add}</p>
  </div>
  <div frx:block="ims/profiles/list_all">
    <h2>Profiles</h2>
    <table>
      <thead>
        <tr><th>Building</th>
        <th>Profile</th>
        <th>Grade Levels</th>
        <th>Type</th></tr>
      </thead>
      <tbody>
        <tr frx:foreach="*">
          <td>{building}</td>
          <td>{name}</td>
          <td>{grades}</td>
          <td>{profile_type}</td>
        </tr>
      </tbody>
    </table>
  </div>  
</body>
</html>
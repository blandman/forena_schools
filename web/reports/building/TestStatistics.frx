<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<frx:fields>
	<frx:field id="name"
		link="index.php?q=building/MeasureStatistics&amp;bldg_id={parm.bldg_id}&amp;grade_level={parm.grade_level}&amp;measure_id={measure_id}&amp;school_year={school_year}&amp;seq={seq}">
	</frx:field>
	  <frx:field id="grade_name" link="index.php?q=BuildingAnalysis&amp;bldg_id={bldg_id}&amp;grade_level={grade_level}"/> 
</frx:fields>
<title>Test Statistics</title>
</head>
<body>
	<div class="grid_12">
    <div id="building" frx:block="ims/building/building_grade"
      frx:foreach="*">
      <h2>{building.name} - {grade_name}</h2>        
    </div>
		<div id="test" frx:block="ims/tests/test" frx:foreach="*">
			<h3>{test.name}</h3>
		</div>
		<div id="schedule" frx:block="ims/building/stat_schedules">

			<div id="selSched" frx:foreach="*[selected='selected']">
				<div id="schedule" frx:block="ims/tests/schedule" frx:foreach="*">
					<h4>{sched_label} {test_name} {year_label}</h4>
				</div>
				<div
					frx:src="building/TestStatsGraph.svg?bldg_id={bldg_id}&amp;grade_level={grade_level}&amp;test_id={test_id}&amp;school_year={school_year}&amp;seq={seq}"
					frx:renderer="FrxInclude" frx:height="600" frx:width="800" />
				<div id="schedule-stats" frx:block="ims/building/test_statistics">
					<p><strong>Performance Distribution:</strong>  The above graph demonstrates the percentage of students tested performing at each level.</p>
					<table>
						<thead>
							<tr>
								<th>measure</th>
								<th>building average</th>
								<th>district average</th>
								<th>struggling</th>
								<th>below</th>
								<th>met</th>
								<th>exceeded</th>
								<th>percent met standard</th>
							</tr>
      </thead>
      <tbody>
        <tr frx:foreach="*">
          <td>{name}</td>
								<td>{b_score}</td>
								<td>{d_score}</td>
								<td>{bl1_percent}</td>
								<td>{bl2_percent}</td>
								<td>{bl3_percent}</td>
								<td>{bl4_percent}</td>
								<td>{b_percent_met}</td>
        </tr>
      </tbody>
    </table>
				</div>
			</div>
		<div>
		  <h3>Testing Coverage</h3>
			<table>
				<thead>
					<tr>
						<th>schedule</th>
						<th>students covered</th>
					</tr>
				</thead>
				<tbody>
					<tr frx:foreach="*">
						<td>{sched_label} {school_year}</td>
						<td>{total}</td>
					</tr>
				</tbody>
			</table>
		</div>
		</div>
	</div>
</body>
</html>
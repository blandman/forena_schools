<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE root [
<!ENTITY nbsp "&#160;">
]>
<html xmlns:frx="urn:FrxReports">
<head>
<title>Student Assessment Profile</title>
<frx:category>Student</frx:category>
<frx:options />
<frx:fields>
  <frx:field id="@date_taken" format="iso_date" format-string="F d, Y"/> 
</frx:fields>
<frx:parameters>
	<frx:parm id="student_id" />
	<frx:parm id="profile_id"/>
</frx:parameters>
<frx:docgen />
</head>
<body>
	<div frx:block="ims/student/student_demographics" frx:foreach="*">
	<h3>{building}</h3>
	  <h4>{first_name} {middle_name} {last_name}</h4>
		<table >
			<tbody>
				<tr>
					<th width="200">SIS ID</th>
					<td>{sis_id}</td>
					<th width="200">State Student ID</th>
					<td>{state_student_id}</td>
				</tr>
        <tr>
          <th width="200">person ID</th>
          <td>{person_id}</td>
          <th width="200">student id</th>
          <td>{student_id}</td>
        </tr>				<tr>
					<th>Phone</th>
					<td>{phone}</td>
					<th>Address</th>
					<td>{address_street} {address_city}, {address_state}  {address_zip}</td>
				</tr>
				<tr>
					<th>Login</th>
					<td>{login}</td>
					<th>E-Mail</th>
					<td>{email}</td>
				</tr>
				<tr>
					<th>Birth Date</th>
					<td>{birthdate}</td>
				</tr>
			</tbody>
		</table>
		<h3>{grade}</h3>
	</div>
	<div frx:block="ims/student/profile_scores">
	  <div frx:foreach="*">
	    <h3>{name}</h3>
	    <div class="test" frx:foreach="tests/assessment">
	      <h4>{@name}</h4>
	      <p>{@date_taken}</p>
		    <table>
		      <tr frx:foreach="measures/measure">
		        <td width="200">{@name}</td>
		        <td class="score" width="90">{@score}</td>
		      </tr>
		    </table>
		    <div frx:foreach="strands[test/measure]" >
          <h4>Detail</h4>
		      <div frx:foreach="test">
			      <p>&nbsp;</p>
			      <table class="test-scores">
			        <tr frx:foreach="measure">
			          <td width="200" class="{@score_class}">{@name}</td>
			          <td class="score" width="90">{@score}</td>
			        </tr>
			      </table>
		      </div>
		    </div>
	    </div>
	  </div>
	</div>
</body>
</html>
